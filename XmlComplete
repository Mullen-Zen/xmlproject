<?xml version="1.0"?>
<!DOCTYPE redcross [

    <!ELEMENT redcross (Account+)>
    <!ELEMENT Account (emailAddress,password,name,address,applicant?,bloodDonor*)>
    <!ELEMENT emailAddress (#PCDATA)>
    <!ELEMENT password (#PCDATA)>
    <!ELEMENT name (#PCDATA)>
    <!ELEMENT address (#PCDATA)>
    <!ELEMENT applicant (CV)>
    <!ELEMENT bloodDonor (dateOfBirth,phoneNumber,medicalInfo)>

    <!ELEMENT CV (#PCDATA)>

    <!ELEMENT  dateOfBirth (#PCDATA)>
    <!ELEMENT  phoneNumber (#PCDATA)>
    <!ELEMENT  medicalInfo (#PCDATA)>




    <!ATTLIST Account id CDATA #REQUIRED>
    <!ATTLIST Account id CDATA #REQUIRED>
    

]>
<redcross>



